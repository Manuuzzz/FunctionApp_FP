
{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
       
        "environment": {
            "value": "Production"
        },
        "functionAppPlanName": {
            "value": "awe-inappsvcplan-azfconsumption-01"
        },
        
        "location": {
            "value": "West Europe"
        },

        "umi": {
            "value": "powerbeextractauditlogs"
            
        },
        "rg_functionAppPlan": {
            "value": "awe-in-rg-opsazfunctions-01"
        },

       "kv": {
                "value": 
        
                {
                    "skuName": "standard",
                    "skuFamily": "A",
                    "enabledForDeployment": true,
                    "enabledForDiskEncryption": false,
                    "enabledForTemplateDeployment": true,
                    "enableSoftDelete": true,
                    "softDeleteRetentionInDays": 90,
                    "enableRbacAuthorization": true
                }
            },
            "sta": {
                "value": 
    
                    {
                        "kind": "StorageV2",
                        "sku": "Standard_LRS",
                        "tlsVersion":"TLS1_2",
                        "blobPublicAccess":true,
                        "sharedKeyAccess": true,
                        "defaultAction": "Allow",
                        "bypass": "AzureServices",
                        "httpsTrafficOnly": true,
                        "allowBlobPublicAccess": true,
                        "accessTier": "Hot",
                        "allowSharedKeyAccess": true,
                        "containerDeleteRetentionPolicy": true,
                        "containerDeleteRetentionPolicyDays": 7,
                        "deleteRetentionPolicy": true,
                        "deleteRetentionPolicyDays": 7,
                        "container1": "powerbilogs",
                        "container2": "powerbiarchivelogs",
                        "byPass": "AzureServices"
                                           
                    }
                },
                "fa": {
                    "value":
                        {
                            "functionAppName": "awe-in-azf-powerbilogs-01",
                            "kind": "functionapp",
                            "enabled": true,
                            "containerSize": "1536"
                            
                            
                        }
                    }
        
    }
}